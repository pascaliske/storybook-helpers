{
  "name": "@pascaliske/storybook-helpers",
  "description": "",
  "version": "1.1.0",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/npm-package.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/npm-package/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts",
    "watch": "rollup --config rollup.config.ts --watch",
    "docs": "typedoc --mode modules --out docs --excludeNotExported --excludeNotDocumented src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest --config jest.config.json --coverage --passWithNoTests && codecov",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "version": "bump --yes package.json",
    "changelog": "standard-changelog",
    "link": "yarn run build && yarn link",
    "unlink": "yarn unlink"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@pascaliske/prettier-config",
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@pascaliske/eslint-config": "^2.0.0",
    "@pascaliske/prettier-config": "^1.1.0",
    "@pascaliske/tsconfig": "^1.0.4",
    "@storybook/addons": "^5.3.21",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "bump-cli": "^1.1.3",
    "codecov": "^3.8.1",
    "eslint": "^7.18.0",
    "expect": "^26.6.2",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rollup": "^2.38.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  }
}
